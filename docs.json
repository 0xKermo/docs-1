{
  "$schema": "https://mintlify.com/schema.json",
  "theme": "mint",
  "name": "Fibrous",
  "logo": {
    "dark": "/logo/dark.png",
    "light": "/logo/light.png"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:contact@fibrous.finance"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Go to App",
      "href": "https://app.fibrous.finance"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "inspiration-for-aggregator",
              "faq"
            ]
          },
          {
            "group": "Quickstart",
            "pages": [
              "getting-started/editing",
              "getting-started/global-settings",
              "getting-started/navigation",
              "getting-started/themes",
              "getting-started/migration"
            ]
          }
        ]
      },
      {
        "tab": "Fibrous Solutions",
        "groups": [
          {
            "group": "User Guides",
            "pages": [
              "solutions/connecting-wallet",
              "solutions/swapping",
              "solutions/price-impact",
              "solutions/block-explorer",
              "solutions/receiving-swaps",
              "solutions/display-settings",
              "solutions/custom-tokens"
            ]
          },
          {
            "group": "Advanced Features",
            "pages": [
              "solutions/batch-swap",
              "solutions/arena"
            ]
          }
        ]
      },
      {
        "tab": "Developer Resources",
        "groups": [
          {
            "group": "API References",
            "pages": [
              "developer/playground",
              "developer/openapi",
              "developer/mdx"
            ]
          },
          {
            "group": "Smart Contracts",
            "pages": [
              "developer/starknet-v2",
              "developer/scroll-router"
            ]
          }
        ]
      }
    ]
  },
  "global": {
    "anchors": [
      {
        "anchor": "App",
        "href": "https://app.fibrous.finance",
        "icon": "arrow-up-right"
      },
      {
        "anchor": "GitHub",
        "href": "https://github.com/fibrous-finance",
        "icon": "github"
      }
    ],
    "dropdowns": [
      {
        "anchor": "Community",
        "icon": "users",
        "items": [
          {
            "anchor": "Discord",
            "href": "https://discord.gg/fibrous",
            "icon": "discord"
          },
          {
            "anchor": "Twitter",
            "href": "https://x.com/fibrousfinance",
            "icon": "twitter"
          }
        ]
      }
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/fibrousfinance",
      "github": "https://github.com/fibrous-finance",
      "discord": "https://discord.gg/fibrous"
    }
  }
}